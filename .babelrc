{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "node": "8.9"
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        ["@babel/plugin-proposal-class-properties", { "loose": false }],
        "@babel/plugin-proposal-object-rest-spread",
        "babel-plugin-add-module-exports"
    ]
}
