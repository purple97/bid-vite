{
    "name": "bid-vite",
    "version": "0.1.0",
    "description": "fed tools",
    "main": "./lib/index.js",
    "scripts": {
        "watch": "npx babel ./src -d lib --watch",
        "delete": "rm -rf lib && rm -rf dist",
        "build": "npm run delete && npx babel ./src -d lib",
        "tag": "npm run build && npm publish ./",
        "release": "np --no-cleanup --yolo --no-publish",
        "prepublishOnly": "yarn test && yarn build",
        "lint": "eslint -c .eslintrc.js --ext .ts ./src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/purple97/bid-vite.git"
    },
    "keywords": [],
    "author": "dezhao.chen",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/purple97/bid-vite/issues"
    },
    "homepage": "https://github.com/purple97/bid-vite#readme",
    "files": [
        "dist",
        "lib"
    ],
    "dependencies": {
        "@babel/runtime": "^7.15.3",
        "cheerio": "^1.0.0-rc.10",
        "cli-color": "^2.0.0",
        "commander": "^5.1.0",
        "eslint": "^7.32.0",
        "git-tools": "^0.3.0",
        "inquirer": "^8.1.2",
        "npm": "^7.20.5",
        "semver": "^7.3.5",
        "urllib": "^2.37.3",
        "wide-align": "^1.1.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.3",
        "@babel/core": "^7.11.1",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/plugin-proposal-decorators": "7.12.1",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
        "@babel/plugin-proposal-optional-chaining": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.10.5",
        "@babel/preset-env": "^7.4.3",
        "babel-eslint": "^10.1.0",
        "babel-plugin-add-module-exports": "^1.0.4",
        "father-build": "1.18.5",
        "jest": "^25.2.2",
        "prettier": "^2.1.2",
        "rimraf": "^3.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.test.ts"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/fixtures/"
        ],
        "transform": {
            "^.+\\.ts$": "<rootDir>/node_modules/ts-jest"
        },
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/__tests__/",
            "/dist/"
        ]
    }
}
